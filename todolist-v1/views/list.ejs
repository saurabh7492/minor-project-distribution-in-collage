 <%- include('header'); -%>
  <div class="box" id="heading">
    <h1><%=listtitle %></h1>
  </div>
  <div class="box">

    <%  for(let i=0;i<newlistitems.length;i++){ %>
    <div class="item">
      <input type="checkbox" name="" value="">
      <p><%= newlistitems[i] %></p>
      </div>
    <%} %>

    <form class="" action="/" method="post">
      <input type="text" name="dolist" value="" placeholder="New Item " autocomplete="off"  required>
      <button type="submit" name="list" value=" <%=listtitle%>">+</button>
    </form>


  </div>
<%- include('footer');-%>
