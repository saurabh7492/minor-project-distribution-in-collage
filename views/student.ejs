<%-include("partials/header");-%>
<div class="row  py-0 px-0">
  <div class="col ">
    <nav class="navbar fixed-top navbar-dark bg-dark navbar-expand-lg bg-light  " >
      <a href="#" class="navbar-brand">
        <img class="img-fluid" height="40px" width="40px" src="images/manipal_university_logo.png" alt="">
      </a>
      <button class="navbar-toggler" data-toggle="collapse" data-target="#mynavbar">
        <span class="navbar-toggler-icon"> </span>
      </button>
      <div class="collapse navbar-collapse" id="mynavbar">
        <ul class="navbar-nav ml-auto">
          <li class="nav-item active ml-1rem"><a href="#" class="nav-link">Home</a></li>
          <li class="nav-item "><a  class="nav-link"href="">Dashbord</a>
          </li>
          <li class="nav-item"><a href="#" class="nav-link"> Your  Project  </a></li>
          <li class="nav-item"><a href="#" class="nav-link">  </a></li>
        </ul>
      </div>
    </nav>
  </div>
</div>
<div class="container">
  <h1 class="mt-5 p-5 text-center"> Select Project   </h1>
   <div class="row">

  <div class="">

 <% if(posts!=null){ %>
<table class="table table-dark  table-hover table-sm table-responsive">
  <thead  class=" ">

  <tr>
    <th scope="col">Faculity Name </th>
    <th scope="col"> Desigination</th>
      <!-- <th scope="col"> Faculity Mobile Number </th> -->
    <th scope="col"> Project Title</th>
    <th scope="col"> Brief Description</th>
    <th scope="col">Objective Of Project</th>
    <th scope="col">Skill Expected From The Students For Project</th>
    <th scope="col"> Faculity Mobile Number </th>



  </tr>
  </thead>
    <%posts.forEach(function(post){%>
      <tbody>

  <tr>

  <form class="" action="/studentpost" method="post">




   <th scope="row"></th>
      <td><%=post.faculitySchema%></td>
      <td><%=post.Degination %></td>

      <td><%=post.ProjectTitle%></td>
      <td><%=post.briefDescription%></td>
      <td><%=post.objectiveProject%></td>
      <td><%=post.skillRequired%></td>
       <td> <%=post.faculitymobilenumber %></td>

      <td>
        <div class=" form-group">
         <input class="form-control" type="text" placeholder="Your Name " size="40" name="name1" value="" required>
        </div>
        <div class="form-group">
          <input class="form-control" type="text" placeholder="Project Partner Nmae " size="20" name="name2"  value="" required>
        </div>
        <div class="form-group">
     <input class="form-control" type="tel"placeholder="enter your mobile number " name="mobilenumber"  size="20" value="" required>
        </div>
        <!-- <input type="text" placeholder="enter your name " name="name1" value="">
        <input type="text"placeholder="enter your projct partener name " name="name2" value="">
        <input type="tel"placeholder="enter your mobile number " name="mobilenumber" value=""> -->
       <input    type="checkbox" id="checked"  onChange="this.form.submit()"name="checkbox" value="<%=post._id%>">
     </td>


</form>
 </tr>
 </tbody>
  <%  }) %>
</table>
<%}%>

</div>
</div>
</div>

<%-include("partials/footer");-%>
